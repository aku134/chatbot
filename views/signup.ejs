<title> Signup </title>
<h1> Signup! </h1>
<% if (warn == true) { %>
    <p> Username already exists! </p>
<% } %>
<form method="POST">
    <input name="username" placeholder="username" required>
    <input type="password" name="password" placeholder="password" required>
    <input name="signup" type="submit" value="signup">
</form>
